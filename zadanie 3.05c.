#include <stdio.h>

int main() {
    int a, b, c = 1;

    scanf("%d %d", &a, &b);


    while (1) {
        int wynik = 1;
        for (int i = 0; i < c; i++)
    {
            wynik *= a;
            }
            if (wynik > b) {
                break;
            }
                c++;
        }
        printf("%d\n", c);

        return 0;
}
